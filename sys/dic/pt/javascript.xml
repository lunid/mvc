<?xml version="1.0" encoding="UTF-8"?>
<root>
    <Site>
        <changeArea>
            <msg id="CATCH">Erro ao alterar área de assinante</msg>
        </changeArea>
    </Site>
    <Slider>
        <init>
            <msg id="CATCH">Erro ao iniciar Slider:</msg>
            <msg id="ERROR_ID">ID do objeto slider não definido!</msg>
        </init>
    </Slider>
    <Menu>
        <default>
            <msg id="ERROR_ID_MENU">ID do objeto menu não definido!</msg>
        </default>
        <init>
            <msg id="CATCH">Erro ao iniciar menu:</msg>
        </init>
        <changeMenu>
            <msg id="CATCH">Erro ao selecionar menu:</msg>
            <msg id="ERROR_ID_ITEM">ID do item de menu não definido!</msg>
        </changeMenu>
    </Menu>
    <Dropdown>
        <init>
            <msg id="CATCH">Erro ao iniciar menu dropdown:</msg>
        </init>
    </Dropdown>
    <Form>
        <init>
            <msg id="CATCH">Erro ao iniciar Formulário:</msg>
            <msg id="ERROR_ID">ID do formulário inválido ou nulo</msg>
            <msg id="FIELD_REQUIRED">O campo %%NAME%% é obrigatório</msg>
            <msg id="FIELD_EMAIL">Digite um e-mail válido</msg>
            <msg id="FIELD_REPEAT">O campo %%NAME%% deve ser igual ao campo %%NAME_ORIGIN%%</msg>
        </init>
        <validate>
            <msg id="CATCH">Erro na validação do Formulário:</msg>
        </validate>
    </Form>    
</root>