<?xml version="1.0" encoding="UTF-8"?>
<!-- 
        URLs:
        baseUrl: não é necessário incluir a barra (/) antes e depois.
        No exemplo a seguir todos os formatos são válidos: 
            app/site
            /app/site/
            /app/site
            app/site/
            
        Caso nenhum valor seja definido, public_html será adotado como valor padrão.
        -->  
<root>
    <params>
        <param id='rootFolder'>mvc</param>           
        <param id='baseUrlHttp'>localhost</param>
        <param id='baseUrlHttps'></param>
    </params>
</root>
